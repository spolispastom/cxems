/**
 * Check if a field is blank
 */
function isBlank(s){
  if ((s =3D=3D=3D null) || (s.length =3D=3D=3D 0)){
    return true;
  }

  for (var i =3D 0; i < s.length; i++){
    var c =3D s.charAt(i);
	  if ((c !=3D ' ') && (c !=3D '\n') && (c !=3D '\t')){
	    return false;
    }
  }
  return true;
}

/**
 * Validate an input field
 */
function validate(form){
    if(!form) return;

    if (isBlank(form.name.value)){
        form.name.focus();
        form.name.style.backgroundColor =3D '#fcc';
        return false;
    } else {
        form.name.style.backgroundColor =3D '#fff';
    }
    if (isBlank(form.mail.value) || form.mail.value.indexOf("@") =3D=3D =
-1){
        form.mail.focus();
        form.mail.style.backgroundColor =3D '#fcc';
        return false;
    } else {
        form.mail.style.backgroundColor =3D '#fff';
    }
    if (isBlank(form.text.value)){
        form.text.focus();
        form.text.style.borderColor =3D '#fcc';
        return false;
    }
}

