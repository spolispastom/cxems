}

#link__wiz_result div.type_d {
    padding: 3px 3px 3px 22px;
    background: transparent url(/lib/tpl/arctic/../../images/ns.png) 3px=
 3px no-repeat;
}

#link__wiz_result div.even {
    background-color: #f5f5f5;
