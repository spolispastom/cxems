    height: 193px;
    overflow: auto;
    border: 1px solid #ccc;
    margin: 3px auto;
    text-align: left;
}

#link__wiz_result div.type_u {
    padding: 3px 3px 3px 22px;
    background: transparent url(/lib/tpl/arctic/../../images/up.png) 3px=
