 */

/* external link */
a.urlextern:after {
   content: " [" attr(href) "]";
   font-size: 90%;
}

/* interwiki link */
a.interwiki:after {
   content: " [" attr(href) "]";
   font-size: 90%;
}

/* email link */
a.mail:after {
   content: " [" attr(href) "]";
   font-size: 90%;
}

