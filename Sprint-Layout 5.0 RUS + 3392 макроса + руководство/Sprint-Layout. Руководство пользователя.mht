    display:none;
    border: 1px solid #ccc;
    position: absolute;
    width:270px;
}

#media__popup h1 {
    text-align:center;
    font-weight:normal;
    background-color: #dee7ec;
    height: 16px;
    margin-bottom: 5px;
    font-size:12px;
    border-bottom: 0;
}

